{
  "name": "xboard-integ-tests",
  "version": "1.0.0",
  "description": "Integration tests for XBoard git project",
  "main": "index.js",
  "scripts": {
    "cy": "cypress run",
    "cy:open": "cypress open",

    "test": "cypress run --config-file cypress.config.js",
    "test-headed": "cypress run --headed --config-file cypress.config.js",
    "cy:headless": "cypress run --headless",
    "ci": "cypress run --headless --browser chrome"
  },
  "keywords": [
    "cypress",
    "e2e",
    "ui",
    "test",
    "test-headed",
    "automation"
  ],
  "author": "Brine",
  "license": "MIT",
  "devDependencies": {
    "cypress-multi-reporters": "^1.4.0",
    "mocha": "^8.3.0",
    "mocha-junit-reporter": "^2.0.0"
  },
  "dependencies": {
    "cypress": "^13.3.1",
    "http-server": "^0.12.3"
  }
}
